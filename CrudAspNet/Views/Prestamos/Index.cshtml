@model IEnumerable<CrudAspNet.Models.Prestamos>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idPrestamo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idLibro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaPrestamo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaDevolucion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombreUsuario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idPrestamo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idLibro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaPrestamo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaDevolucion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombreUsuario)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.idPrestamo }) |
            @Html.ActionLink("Details", "Details", new { id = item.idPrestamo }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.idPrestamo })
        </td>
    </tr>
}

</table>
